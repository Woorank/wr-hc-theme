<nav class="user-nav">
  {{user_info}}
  <a class="back-to-woorank" href="https://help.woorank.com/hc/en-us" title="Back to Wooorank">
  	<img src="{{asset 'woorank-help-dark-smaller.png'}}">
  </a>
  {{#if alternative_locales}}
    <div class="dropdown language-selector" aria-haspopup="true">
      <a class="dropdown-toggle">
        {{current_locale.name}}
      </a>
      <span class="dropdown-menu dropdown-menu-end" role="menu">
        {{#each alternative_locales}}
          <a class="back-to-woorank" href="{{url}}" dir="{{direction}}" rel="nofollow" role="menuitem">
            {{name}}
          </a>
        {{/each}}
      </span>
    </div>
  {{/if}}
</nav>
<header class="header">
  <div class="header-inner">
    <div class="logo">{{#link 'help_center'}}
      <img src="{{logo_url}}" alt="{{t 'logo'}}">
    {{/link}}</div>
  </div>
</header>